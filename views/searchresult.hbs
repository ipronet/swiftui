<script type="text/javascript">

function updateRecord(myform)
{
    var y = document.forms.namedItem(myform);
    var myForm = $('form#'+myform);

    $.post('/updateprofile', myForm.serialize(), function(resp) {

                if(resp.processed)
                {
                    var x = document.forms.namedItem(myform);
                    x.elements.account_name.value = resp.account_name;
                    document.getElementById(myform+"tr").cells[x.elements.account_name.id].innerHTML=resp.account_name;
                    x.elements.account_email.value = resp.account_email;
                    document.getElementById(myform+"tr").cells[x.elements.account_email.id].innerHTML=resp.account_email;
                    x.elements.account_tel.value = resp.account_tel;
                    document.getElementById(myform+"tr").cells[x.elements.account_tel.id].innerHTML=resp.account_tel;
                    x.elements.account_number.value = resp.account_number;
                    document.getElementById(myform+"tr").cells[x.elements.account_number.id].innerHTML=resp.account_number;
                    //x.elements.account_mtdata.value = resp.account_mtdata;
                    //document.getElementById("{{@key}}tr").cells[x.elements.account_mtdata.id].innerHTML=  x.elements.account_mtdata.value.innerHTML;  
                    
                    Materialize.toast('Record Updated Successfully!', 5000);
                    swal("Updated", "Record Updated Successfully!", "success");

                }
                else{
                    Materialize.toast('Error!!! Record no updated.', 5000);
                    swal("Error", "Error Updating Record", "error");

                }

            });
            $('#'+myform+'close').click();
            $('#'+myform+'close').trigger("click");

    //$('#dialog'+myform).removeClass('open');
    //$('#dialog'+myform).css('display', 'none');
    //$('#dialog'+myform).css('opacity', '0');
    //$('#dialog'+myform).css('transform', 'scaleX(0.7)');
    //$("body").removeAttr("style");
    //$('.lean-overlay').remove();
}


