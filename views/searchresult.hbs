<!-- Modal Trigger   visibility   -->

{{#each tabledata}}
<!-- Modal Structure -->
<div id="dialog{{@key}}" class="modal modal-fixed-footer">
    <form name="{{@key}}" id="{{@key}}" action="javascript:void(0);" >
    <div class="modal-content">
        <h4 class="center-align">RECORD: {{this.AccountName}}</h4>
        
        

            <div class="row">

                <div class="row"></div>

                <div class="col s12 m12 l4">

                    <div class="row">
                        <div class="input-field col s12">
                            <input id="account_name" name="account_name" type="text" class="validate" value="{{this.AccountName}}">
                            <label for="account_name">Account Name</label>
                        </div>
                    </div>
                    <div class="row">
                        <div class="input-field col s12">
                            <input id="account_email" name="account_email" type="email" class="validate" value="{{this.AccountEmail}}">
                            <label for="account_email">Account Email</label>
                        </div>
                    </div>
                    <div class="row">
                        <div class="input-field col s12">
                            <input id="account_tel" name="account_tel" type="text" class="validate" value="{{this.AccountTelephone}}"> 
                            <label for="account_tel">Account Telephone</label>
                        </div>
                    </div>

                </div>
                <div class="col s12 m12 l4">

                    <div class="row">
                        <div class="input-field col s12">
                            <input id="account_number" name="account_number" type="number" class="validate" value="{{this.AccountNumber}}">
                            <label for="account_number">Account Number</label>
                        </div>
                    </div>
    <div class="row">
        <div class="input-field col s12">
            <select name="account_mtdata" id="account_mtdata">
                <option value="" disabled selected>Select Message Type</option>
               
               {{#each ../mtdata}}
                <option value="{{this.val}}" {{isSelected ../MT this.val}} >{{this.name}}</option>
                {{/each}}
            </select>
            <label>Select Message Type</label>
        </div>

    </div>
                    

    <div class="row">
        <div class="input-field col s12">
            <select name="account_msgflow">
                {{#each ../msgflow}}
                    <option value="{{this.val}}" {{isSelected ../MsgDir this.val}}>{{this.name}}</option>
                {{/each}}
            </select>
            <label>Select Message Direction</label>
        </div>

    </div>

                </div>
            
                <div class="col s12 m12 l4">

                   
    <div class="row">
        <div class="input-field col s12">
            <select name="account_alert_temp">
                <option value="" disabled selected>Select Message Template</option>
                {{#each ../alerttemps}}
                    <option value="{{this.val}}" {{isSelected ../MsgTemp this.val}}>{{this.name}}</option>
                {{/each}}
            </select>
            <label>Select Message Template</label>
        </div>

    </div>
                    

    <div class="row">
        <div class="input-field col s12">
            <select name="account_alertgroups">
                <option value="" disabled selected>Select Internal Alert Group</option>
                {{#each ../alertgroups}}
                    <option value="{{this.val}}" {{isSelected ../AlertGroup this.val}}>{{this.name}}</option>
                {{/each}}
            </select>
            <label>Select Internal Alert Group</label>
        </div>

    </div>

    
    <div class="row">
        <div class="input-field col s12">
            <select name="account_alerttypes">
                <option value="" disabled selected>Select Alert Type</option>
                {{#each ../alerttypes}}
                    <option value="{{this.val}}" {{isSelected ../Alert this.val}}>{{this.name}}</option>
                {{/each}}
            </select>
            <label>Select Alert Type</label>
        </div>

    </div>

                </div>
            
            </div>
        

    </div>


    <div class="modal-footer">
        <div class="col s5"></div> <a class="modal-action waves-effect waves-green btn-flat" onclick="updateRecord('{{@key}}');">Update</a>
        <div class="col s5"></div> <a class="modal-action modal-close waves-effect waves-green btn-flat" id="{{@key}}close">Cancel</a>
        <!--<div class="col s5"></div> <input type="submit" class="btn-flat" onclick="" value="Update Record"> -->
    </div>
    </form>
</div>
{{/each}}


