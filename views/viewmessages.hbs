{{> mtModalsTemp}}




<!-- Modal Trigger -->
<a id="asendmailmodal" class="waves-effect waves-light btn modal-trigger" style="visibility: hidden;"
    href="#sendmailmodal">Modal</a>
<a class="waves-effect waves-light btn modal-trigger" id="showmsgdetails" style="visibility: hidden;"
    href="#msgdetails"></a>

<!-- Modal Structure -->
<div id="sendmailmodal" class="modal">
    <div class="modal-content">
        <h4>Provide Email Addresses</h4>
        <p>
        <div class="input-field col s12">
            <input id="report_email" type="email" class="validate">
            <label for="report_email">Email</label>
        </div>
        <div class="input-field col s12" style="max-height:10em; height:10em">
            <textarea id="report_textarea" class="materialize-textarea"
                style="max-height:8em; overflow-y: scroll;"></textarea>
            <label for="report_textarea">Email Message</label>
        </div>
        {{!-- <input name="report_txtdoc" id="report_txtdoc" type="hidden" /> --}}
        {{!-- <input name="report_form" id="report_form" type="hidden" /> --}}
        </p>
    </div>
    <div class="modal-footer">
        <a class=" modal-action modal-close waves-effect waves-green btn-flat" onClick="sendMail()">Send</a>
    </div>
</div>