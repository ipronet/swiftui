<div class="row">
  <div class="col s12">
    <div class="page-title">FOLDER SETTINGS</div>
  </div>

  <div class="col s12 m12 l12">
    <div class="card">
      <div class="card-content">
        <div class="row">
          <div class="col s12">
            <ul class="tabs tab-demo z-depth-1" style="width: 100%;">
              <li class="tab col s3"><a href="#test1" class="">Specify Paths For
                  Source Files</a></li>
            </ul>
          </div>

          <div id="test1" class="col s12">
            <div class="row"></div>

            <form>
              {{#each list}}
                <div class="row">
                  <div class="col m3 s12 center-align">

                    <img
                      src="../../images/folder.jpg"
                      class="m-b-lg"
                      alt=""
                      width="90%"
                    />
                    <p class="grey-text center-align">-</p>
                  </div>

                  <div class="col s12 m12 l4">
                    <div class="row"></div>
                    <div class="row">
                      <div class="input-field col s12">
                        <input
                          value={{src_swift_docs_path_print}}
                          id="src_swift_docs_path_print"
                          type="text"
                          class="validate"
                          name="src_swift_docs_path_print"
                          placeholder="Enter Print Path"
                        />
                        <label for="src_swift_docs_path_print">Print Path</label>
                      </div>
                    </div>

                    <div class="row">
                      <div class="input-field col s12">
                        <input
                          value={{src_swift_docs_path_pdf}}
                          id="src_swift_docs_path_pdf"
                          type="text"
                          class="validate"
                          name="src_swift_docs_path_pdf"
                          placeholder="Enter Pdf Path"
                        />
                        <label for="src_swift_docs_path_pdf">Pdf Path</label>
                      </div>
                    </div>
                  </div>
                  <div class="col s12 m12 l4">
                    <div class="row"></div>
                    <div class="row">
                      <div class="input-field col s12">
                        <input
                          value={{src_swift_docs_path_img}}
                          id="src_swift_docs_path_img"
                          type="text"
                          class="validate"
                          name="src_swift_docs_path_img"
                          placeholder="Enter Image Path"
                        />
                        <label for="src_swift_docs_path_img">Image Path</label>
                      </div>
                    </div>

                    <div class="row">
                      <div class="input-field col s12">
                        <input
                          id="src_swift_docs_path_txt"
                          type="text"
                          value={{src_swift_docs_path_txt}}
                          class="validate"
                          name="src_swift_docs_path_txt"
                          placeholder="Enter Text
                Path"
                        />
                        <label for="src_swift_docs_path_txt">Text Path</label>
                      </div>
                    </div>
                    <input
                      type="hidden"
                      id="idsettings_paths"
                      name="idsettings_paths"
                      value={{idsettings_paths}}
                    />
                  </div>

                </div>

                <div class="row">
                  <div class="col s5"></div>
                  <button
                    class="waves-effect waves-light btn"
                    type="submit"
                    id="update_path"
                  >Save Settings</button>
                </div>
              {{/each}}
            </form>
          </div>

        </div>

      </div>

    </div>
  </div>
</div>