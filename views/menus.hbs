{{! <main class="mn-inner"> }}
<div class="row">
  <div class="col s12">
    <div class="page-title">SETTINGS / MENU</div>
  </div>
  <div class="col s12 m12 l12">
    <div class="card">
      <div class="card-content">
        <div class="col s12">
          <ul class="tabs tab-demo z-depth-1" style="width: 100%;">
            <li class="tab col s3"><a href="#test1" class="">Menu List</a></li>
          </ul>
        </div>
        <div class="row"></div>
        <div style="width:100%;font-size:12px; overflow-x:scroll;">
        <table id="example" class="display responsive-table datatable-example">
          <thead>
            <tr>
              <th>Action</th>
              <th>Menu Title</th>
              <th>Menu Url</th>
              <th>Menu Icon</th>
              <th>Menu Type</th>
            </tr>
          </thead>
          
          <tbody>
            {{#each list}}
              <tr>
                <td><a href="/settings/menu/{{id}}">
                {{!-- <td><a href="#"> --}}
                    <i
                      style="color: #26A69A;"
                      class="material-icons"
                    >visibility</i>
                  </a></td>
                <td>{{title}}</td>
                <td>{{url}}</td>
                <td>{{icon}}</td>
                <td>{{isBaseId}}</td>
              </tr>
            {{/each}}
          </tbody>
        </table>
        </div>
      </div>
    </div>
  </div>
</div>
{{! </main> }}