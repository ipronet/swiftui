                                            <td id="BankCode">{{this.BankCode}}</td>
                                            <td id="BenAccount">{{this.BenAccount}}</td>
                                            <td id="Valdate">{{this.Valdate}}</td>
                                            <td id="Currency">{{this.Currency}}</td>
                                            <td id="Amount">{{this.Amount}}</td>
                                            <td width="10%" class="center-align">
