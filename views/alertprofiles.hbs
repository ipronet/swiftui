{{! <main class="mn-inner"> }}
<div class="row">
  <div class="col s12">
    <div class="page-title">Alert Profilles</div>
  </div>
  <div class="col s12 m12 l12">
    <div class="card">
      <div class="card-content">
        <div class="col s12">
          <ul class="tabs tab-demo z-depth-1" style="width: 100%;">
            <li class="tab col s3"><a href="#test1" class="">Alert Profiles</a></li>
          </ul>
        </div>
        <div class="row"></div>
        <div style="width:100%;font-size:12px; overflow-x:scroll;">
        <table id="example" class="display responsive-table datatable-example">
          <thead>
            <tr>
              <th>Action</th>

              <th>Customer Name</th>
              <th>Customer Email</th>
              <th>SMS Number</th>
              <th>Account Number</th>
              <th>Message Type</th>
              <th>Message Direction</th>
              <th>Message Template</th>
              <th>Alert Email</th>
              <th>Alert Type</th>
              <th>Status</th>
            </tr>
          </thead>
          {{! <tfoot>
            <tr>
              <th>Name</th>
              <th>Position</th>
              <th>Office</th>
              <th>Age</th>
              <th>Start date</th>
              <th>Salary</th>
            </tr>
          </tfoot> }}
          <tbody>
            {{#each list}}
              <tr>
                <td><a href="/settings/profiledetail/{{email}}">
                    <i
                      style="color: #26A69A;"
                      class="material-icons"
                    >visibility</i>
                  </a></td>
                <td>{{CustomerName}}</td>
                <td>{{email}}</td>
                <td>{{sms}}</td>
                <td>{{account_number}}</td>
                <td>{{idMsgType}}</td>
                <td>{{idFlow}}</td>
                <td>{{message_template}}</td>
                <td>{{internal_alert_email}}</td>
                <td>{{idalert_type}}</td>
                <td>{{status}}</td>
              </tr>
            {{/each}}
          </tbody>
        </table>
        </div>
      </div>
    </div>
  </div>
</div>
{{! </main> }}