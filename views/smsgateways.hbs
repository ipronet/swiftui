{{! <main class="mn-inner"> }}
<div class="row">
  <div class="col s12">
    <div class="page-title">SMS Gateways</div>
  </div>
  <div class="col s12 m12 l12">
    <div class="card">
      <div class="card-content">
        <div class="col s12">
          <ul class="tabs tab-demo z-depth-1" style="width: 100%;">
            <li class="tab col s3"><a href="#test1" class="">SMS Gateways</a></li>
          </ul>
        </div>
        <div class="row"></div>
        <div style="width:100%;font-size:12px; overflow-x:scroll;">
        <table id="example" class="display responsive-table datatable-example">
          <thead>
            <tr>
              <th>Action</th>
              <th>Gateway</th>
              <th>Gateway Url</th>
              <th>Gateway Keys</th>
              <th>Gateway Password</th>
              <th>Balance</th>
              <th>Status</th>
            </tr>
          </thead>
          {{! <tfoot>
            <tr>
              <th>Name</th>
              <th>Position</th>
              <th>Office</th>
              <th>Age</th>
              <th>Start date</th>
              <th>Salary</th>
            </tr>
          </tfoot> }}
          <tbody>
            {{#each list}}
              <tr>
                <td><a href="/settings/smsgateway/{{id}}">
                    <i
                      style="color: #26A69A;"
                      class="material-icons"
                    >visibility</i>
                  </a></td>
                <td>{{gateway}}</td>
                <td>{{gatewayurl}}</td>
                <td>{{gatewaykeys}}</td>
                <td>{{gatewaypassword}}</td>
                <td>{{balance}}</td>
                <td>{{status}}</td>
              </tr>
            {{/each}}
          </tbody>
        </table>
        </div>
      </div>
    </div>
  </div>
</div>
{{! </main> }}